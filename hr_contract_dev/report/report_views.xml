<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="paperformat_contract" model="report.paperformat">
            <field name="name">Contract</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">25</field>
            <field name="margin_bottom">15</field>
            <field name="margin_left">10</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">22</field>
            <field name="dpi">80</field>
        </record>

        <record id="report_kompensasi_print" model="ir.actions.report">
            <field name="name">Kompensasi</field>
            <field name="model">hr.contract</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">hr_contract_dev.report_kompensasi</field>
            <field name="report_file">hr_contract_dev.report_kompensasi</field>
            <field name="print_report_name">'Kompensasi - %s' % (object.employee_id.name)</field>
            <field name="paperformat_id" ref="hr_contract_dev.paperformat_contract"/>
            <field name="binding_model_id" ref="model_hr_contract"/>
            <field name="binding_type">report</field>
        </record>

        <record id="report_contract_print" model="ir.actions.report">
            <field name="name">Contract</field>
            <field name="model">hr.contract</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">hr_contract_dev.report_contract</field>
            <field name="report_file">hr_contract_dev.report_contract</field>
            <field name="print_report_name">'Contract - %s' % (object.employee_id.name)</field>
            <field name="paperformat_id" ref="hr_contract_dev.paperformat_contract"/>
            <field name="binding_model_id" ref="model_hr_contract"/>
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>