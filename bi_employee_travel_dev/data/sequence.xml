<?xml version="1.0" ?>
<odoo>
    <data noupdate='1'>
		<record id="ir_seq_travel_request" model="ir.sequence">
			<field name="name">Travel Request Sequence</field>
			<field name="code">travel.request.seq</field>
			<field name="prefix">BSS/</field>
			<field name="suffix">/ST/%(month)s/%(year)s</field>
			<field name="padding">3</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
		</record>
	</data>
</odoo>

