<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hr_contract_advantage_template_view_form_dev" model="ir.ui.view">
        <field name="model">hr.contract.advantage.template</field>
        <field name="inherit_id" ref="hr_payroll_community.hr_contract_advantage_template_view_form"/>
        <field name="arch" type="xml">
            <field name="code" position="after">
                <field name="sequence"/>
            </field>
        </field>
    </record>

    <record id="hr_contract_advantage_template_view_tree_dev" model="ir.ui.view">
        <field name="model">hr.contract.advantage.template</field>
        <field name="inherit_id" ref="hr_payroll_community.hr_contract_advantage_template_view_tree"/>
        <field name="arch" type="xml">
            <field name="name" position="before">
                <field name="sequence" widget="handle"/>
            </field>
        </field>
    </record>
</odoo>