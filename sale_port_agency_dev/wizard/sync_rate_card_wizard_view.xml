<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sync_rate_card_wizard_view_form" model="ir.ui.view">
        <field name="name">sync.rate.card.wizard.view.form</field>
        <field name="model">sync.rate.card.wizard</field>
        <field name="arch" type="xml">
            <form string="Sync. Cost Structure">
                <group>
                    <group>
                        <field name="cost_structure_id"/>
                        <field name="order_id" invisible="1"/>
                    </group>
                </group>
                <footer>
                    <button name="sync_rate_card" string="Synchronize" type="object" class="oe_highlight" data-hotkey="s"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="x"/>
                </footer>
            </form>
        </field>
    </record>

