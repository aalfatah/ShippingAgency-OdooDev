<?xml version="1.0" ?>
<odoo>
    <data noupdate='0'>
		<record id="ir_seq_employee_resignation" model="ir.sequence">
			<field name="name">Employee_resignation</field>
			<field name="code">employeeresignation.seq</field>
			<field name="prefix">BSS/</field>
			<field name="padding">4</field>
			<field name="suffix">/type/%(rom_month)s/%(year)s</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
		</record>

		<record id="ir_seq_hr_employee_resignation" model="ir.sequence">
			<field name="name">HR Employee_resignation</field>
			<field name="code">hr.employeeresignation.seq</field>
			<field name="prefix">BSS/</field>
			<field name="padding">4</field>
			<field name="suffix">/format</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
		</record>

	</data>
</odoo>

